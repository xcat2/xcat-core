end


start:tabdump_d
label:mn_only,ci_test,db
cmd:tabdump -d |grep site
check:rc==0
check:output=~site:
end

start:tabdump_f_d
label:mn_only,ci_test,db
cmd:tabdump -f /tmp/table -d site|grep site
check:rc==0
check:output=~site
cmd:rm -f /tmp/table
end

start:tabdump_d_nodehm
label:mn_only,ci_test,db
cmd:tabdump -d site | grep xcatiport
check:rc==0
check:output=~xcatiport
end

start:tabdump_servicenode
label:mn_only,ci_test,db
cmd:tabdump servicenode
check:rc==0
end
