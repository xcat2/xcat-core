cmd:lsdef -l $$CN | grep status
check:output=~booted
start:test_log
cmd:lsdef -l $$CN | grep status
check:output=~booted
cmd:xdsh $$CN "cat  /var/log/xcat/xcat.log | grep \"Running preseeding early\_command Installation script\""
check:output=~Running preseeding early\_command Installation script
